DROP TABLE `Comentarios`;

CREATE TABLE `Comentarios` (
  `fecha` datetime NOT NULL,
  `comentario` text COLLATE utf8_spanish_ci NOT NULL,
  `idUsuario` int(11) NOT NULL,
  `idIncidencia` int(11) NOT NULL,
  `id` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci COMMENT='Comentarios de las incidencias.';

DROP TABLE `FotosIncidencias`;

CREATE TABLE `FotosIncidencias` (
  `rutaImagen` varchar(40) COLLATE utf8_spanish_ci NOT NULL,
  `idIncidencia` int(11) NOT NULL,
  `idImagen` varchar(20) COLLATE utf8_spanish_ci NOT NULL,
  PRIMARY KEY (`rutaImagen`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci COMMENT='Fotografias de las incidencias';

DROP TABLE `Incidencias`;

CREATE TABLE `Incidencias` (
  `titulo` varchar(50) COLLATE utf8_spanish_ci NOT NULL,
  `lugar` varchar(50) COLLATE utf8_spanish_ci NOT NULL,
  `fecha` datetime NOT NULL,
  `palabrasClave` varchar(100) COLLATE utf8_spanish_ci NOT NULL,
  `estado` varchar(15) COLLATE utf8_spanish_ci NOT NULL COMMENT '''Pendiente'', ''Comprobada'', ''Tramitada'', ''Irresoluble'' o ''Resuelta''',
  `positivas` int(11) NOT NULL,
  `negativas` int(11) NOT NULL,
  `descripcion` text COLLATE utf8_spanish_ci NOT NULL,
  `id` int(11) NOT NULL,
  `idUsuario` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci COMMENT='Incidencias del municipio';

DROP TABLE `Logs`;

CREATE TABLE `Logs` (
  `fecha` datetime NOT NULL,
  `accion` varchar(200) COLLATE utf8_spanish_ci NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci;

DROP TABLE `Usuarios`;

CREATE TABLE `Usuarios` (
  `nombre` varchar(40) COLLATE utf8_spanish_ci NOT NULL,
  `apellidos` varchar(60) COLLATE utf8_spanish_ci NOT NULL,
  `clave` varchar(100) COLLATE utf8_spanish_ci NOT NULL,
  `email` varchar(30) COLLATE utf8_spanish_ci NOT NULL,
  `direccion` varchar(100) COLLATE utf8_spanish_ci NOT NULL,
  `telefono` int(11) NOT NULL,
  `rol` varchar(15) COLLATE utf8_spanish_ci NOT NULL COMMENT 'Colaborador o Administrador',
  `estado` varchar(15) COLLATE utf8_spanish_ci NOT NULL COMMENT 'Activo o Pendiente',
  `imagen` varchar(40) COLLATE utf8_spanish_ci NOT NULL COMMENT 'Ruta de la imagen',
  `id` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `email` (`email`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci COMMENT='Datos de los usuarios.';

DROP TABLE `Valoraciones`;

CREATE TABLE `Valoraciones` (
  `idUsuario` int(11) NOT NULL,
  `idIncidencia` int(11) NOT NULL,
  `tipo` varchar(7) COLLATE utf8_spanish_ci NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci;

